{
  "name": "automation-testing-framework",
  "version": "1.0.0",
  "description": "Advanced automation testing framework with PostgreSQL data analytics and business intelligence",
  "main": "data_analysis.py",
  "repository": {
    "type": "git",
    "url": "https://github.com/BeGoodAde/automation-testing-framework.git"
  },
  "author": "BeGoodAde",
  "license": "MIT",
  "homepage": "https://github.com/BeGoodAde/automation-testing-framework#readme",
  "bugs": {
    "url": "https://github.com/BeGoodAde/automation-testing-framework/issues"
  },
  "scripts": {
    "test": "jest",
    "test:python": "python -m pytest tests/ -v",
    "test:coverage": "python -m pytest --cov=. --cov-report=html",
    "test:all": "npm run test:python && npm test",
    "lint": "eslint . && flake8 .",
    "format": "prettier --write . && black .",
    "prepare": "npm run test:all"
  },
  "keywords": [
    "automation-testing",
    "data-analytics", 
    "postgresql",
    "business-intelligence",
    "python",
    "javascript",
    "pytest",
    "jest",
    "sql-analytics",
    "data-processing"
  ],
  "devDependencies": {
    "jest": "^29.6.2",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0",
    "@babel/preset-env": "^7.22.9",
    "babel-jest": "^29.6.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "python": ">=3.8.0"
  }
}